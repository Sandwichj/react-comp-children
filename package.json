{
  "name": "react-comp-children",
  "version": "0.3.1",
  "description": "A react component that makes easy to mount children dynamically.",
  "main": "index.js",
  "scripts": {
    "prepare:prod": "cross-env NODE_ENV=production babel src/Children.js --out-file build/react-comp-children.production.js",
    "prepare:dev": "cross-env NODE_ENV=development babel src/Children.js --out-file build/react-comp-children.development.js",
    "prepare": "(npm run prepare:prod) && (npm run prepare:dev)",
    "test": "jest",
    "eslint": "eslint ./src --ext .js --ext .jsx --cache || true"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-config-google": "^0.9.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "jest": {
    "verbose": true,
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/lib/**",
      "!**/__test__/**",
      "!**/node_modules/**",
      "!**/projects/**"
    ],
    "coverageDirectory": "./src/__test__/__coverage__",
    "coverageThreshold": {
      "src/**/*.{js,jsx}": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sandwichj/react-comp-children.git"
  },
  "keywords": [
    "react",
    "comp",
    "component",
    "child",
    "children",
    "select",
    "mount"
  ],
  "author": "msjeong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sandwichj/react-comp-children/issues"
  },
  "homepage": "https://github.com/Sandwichj/react-comp-children#readme"
}
